{
  "name": "markdown-to-html",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec \"vite-node ./src/index.ts & prettier --write ./public\" -e md,ts,css",
    "start": "serve ./public/",
    "build": "npx yarn & vite-node ./src/index.ts",
    "format": "prettier --write ./public"
  },
  "devDependencies": {
    "@types/node": "^20.10.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "serve": "^14.2.1",
    "vite-node": "^0.34.6"
  },
  "dependencies": {
    "front-matter": "^4.0.2",
    "marked": "^11.0.0"
  }
}
